job('First_maven_project_via_DSL') {
    description("my first maven job generated by the dsl")
    scm {
        git('https://github.com/ashusrms14/Tomcat-deployment-calculator.git', 'master')
    }
    triggers {
        scm('* * * * *')
    }
    steps {
        maven('clean package','Tomcat-deployment-calculator/pom.xml')
    }
    publishers {
        archiveArtifacts('**/*.war')
    }
}
